{
  "name": "boilerplate-node-nodejs-spider-crawl-puppeteer",
  "version": "0.0.0",
  "description": "boilerplate node nodejs spider crawl puppeteer",
  "engines": {
    "node": ">= 20.9 <21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "~29.5",
    "@types/node": "^20.9",
    "@types/node-schedule": "^2.1.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "~8.4.0",
    "@typescript-eslint/parser": "~8.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-jest": "~28.8.3",
    "globals": "^15.9.0",
    "javascript-obfuscator": "^4.0.2",
    "jest": "~29.7.0",
    "prettier": "~3.3.3",
    "rimraf": "~6.0.1",
    "ts-jest": "~29.2.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "~3.21",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "scripts": {
    "start": "nodemon",
    "dev": "yarn run swagger && nodemon",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "swagger": "yarn run tsoa spec && tsoa routes",
    "swagger route": "yarn run tsoa routes",
    "format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix ",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^8.6.1",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.0.1",
    "puppeteer": "^23.3.0",
    "swagger-ui-express": "^5.0.0",
    "tslib": "~2.7.0",
    "tsoa": "^6.4.0",
    "validator": "^13.9.0"
  },
  "volta": {
    "node": "18.17.0"
  }
}
